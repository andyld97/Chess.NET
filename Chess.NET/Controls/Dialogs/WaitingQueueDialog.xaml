<Window x:Class="Chess.NET.Controls.Dialogs.WaitingQueueDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Chess.NET.Controls.Dialogs"
        mc:Ignorable="d" WindowStartupLocation="CenterOwner" ResizeMode="NoResize"
        xmlns:res="clr-namespace:Chess.NET.Properties"
        Title="{x:Static res:Resources.strWaiting_Title}" Height="150" Width="400" Closing="Window_Closing">
    <Grid>
        <TextBlock Text="{x:Static res:Resources.strWaiting_Content}" HorizontalAlignment="Center" VerticalAlignment="Center" FontWeight="Bold" FontSize="28" />
    </Grid>
</Window>
